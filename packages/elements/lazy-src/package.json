{
	"name": "@currentjs/element-library_elments_lazy-src",
	"version": "0.1.0",
	"description": "An element that will lazy load any data-src attribute on the first child element",
	"author": "@currentjs",
	"homepage": "https://github.com/currentjs/element-library",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/currentjs/element-library.git",
		"directory": "packages/elements/lazy-src"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/",
		"access": "restricted"
	},
	"scripts": {
		"test": "karma start ../../../karma.conf.js --grep \"packages/elements/lazy-src/tests/**/*.test.js\"",
